    "example-retry": "node src/examples/retry-start.js",
    "example-fetch": "node src/examples/example-fetch.js",
    "example-fetch-nourl": "node src/examples/example-fetch-nourl.js",
    "example-lookup": "node src/examples/version-lookup.js",
